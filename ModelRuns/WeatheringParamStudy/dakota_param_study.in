# Dakota Input File
environment
  tabular_data
    tabular_data_file = 'dakota.dat'

method
  multidim_parameter_study
    partitions = 2 2 4

variables
  continuous_design = 3
    lower_bounds = -3 2 -2
    upper_bounds = -1 4 2
    descriptors = 'D' 'U' 'Wprime'

interface
  fork
  analysis_driver = 'parallel_driver.py'
  parameters_file = 'params.in'
  results_file = 'results.out'
  work_directory 
    named 'run'
    directory_tag
    directory_save 
  file_save

responses
  response_functions = 2
  response_descriptors = 'Zmax', 'Smedian'
  no_gradients
  no_hessians
